" Config

" enables fzf for fuzzy search
" requires installation of fzf, ripgrep, universal-ctags, and fd
let enable_fzf = 1

" enables git integration
" required git (which you should have anyway for plug)
let enable_git = 1

" select theme
let theme = "onedark" " Set to 'none' to disable theme

" additional navigation options
let quickscope = 1 " single line navigation
let sneak = 1 " full file navigation
